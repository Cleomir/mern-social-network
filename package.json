{
  "name": "mern-social-network",
  "version": "1.0.0",
  "description": "A MERN stack app",
  "main": "index.js",
  "scripts": {
    "start": "node ./server/dist/server-bundle.js",
    "build:dev": "webpack --config ./server/webpack.dev.js",
    "build:prod": "webpack --config ./server/webpack.prod.js",
    "watch": "npm-run-all --parallel watch:build watch:server --print-label",
    "watch:build": "webpack --watch --config ./server/webpack.dev.js",
    "watch:server": "nodemon ./server/dist/server-bundle.js --watch ./server/dist",
    "test": "jest",
    "docs": "npx typedoc"
  },
