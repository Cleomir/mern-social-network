    "gravatar": "^1.8.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.18",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@microsoft/tsdoc": "^0.12.20",
    "@types/ajv": "^1.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.6",
    "@types/gravatar": "^1.8.1",
    "@types/helmet": "0.0.47",
