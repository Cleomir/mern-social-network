{
  "name": "mern-social-network",
  "version": "1.0.0",
  "description": "A MERN stack app",
  "main": "index.js",
  "scripts": {
    "start": "node ./server/dist/server-bundle.js",
    "build:dev": "webpack --config ./server/webpack.dev.js",
    "build:prod": "webpack --config ./server/webpack.prod.js",
    "watch": "npm-run-all --parallel watch:build watch:server --print-label",
    "watch:build": "webpack --watch --config ./server/webpack.dev.js",
    "watch:server": "nodemon ./server/dist/server-bundle.js --watch ./server/dist",
    "test": "jest",
    "docs": "npx typedoc"
  },
  "author": "Cleomir Machado Gomes",
  "license": "MIT",
  "dependencies": {
    "ajv": "^6.12.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.13",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@microsoft/tsdoc": "^0.12.19",
    "@types/ajv": "^1.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.6",
    "@types/gravatar": "^1.8.1",
    "@types/jest": "^24.9.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.19",
    "@types/node": "^10.17.21",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.9",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-node": "^11.1.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "jest": "^26.0.1",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.0",
    "ts-loader": "^5.4.5",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
